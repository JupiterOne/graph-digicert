{
  "name": "@jupiterone/graph-digicert",
  "version": "2.1.0",
  "description": "A graph conversion tool for https://digicert.com",
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "collect": "j1-integration collect",
    "graph": "j1-integration visualize",
    "start": "yarn collect; yarn graph",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "prebuild": "yarn test:ci",
    "prepush": "yarn test:ci",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-core": "^7.0.0",
    "@jupiterone/integration-sdk-dev-tools": "^7.0.0",
    "@jupiterone/integration-sdk-testing": "^7.0.0",
    "jest-fetch-mock": "^3.0.3",
    "wait-for-expect": "^3.0.2"
  },
  "peerDependencies": {
    "@jupiterone/integration-sdk-core": "^7.0.0"
  }
}
